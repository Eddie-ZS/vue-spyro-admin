{
	"name": "spyro-vue-admin",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"serve": "pnpm dev",
		"build:dev": "vite build --mode development",
		"build:prod": "vite build --mode production",
		"build:stag": "vite build --mode staging",
		"build:preview": "vite build --mode production && vite preview",
		"check": "vue-tsc --noEmit --skipLibCheck",
		"preview": "vite preview",
		"lint:eslint": "eslint src --fix",
		"lint:format": "prettier --write --log-level warn \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\"",
		"lint:check": "prettier --check --log-level warn \"src/**/*.{js,ts,json,jsx,tsx,css,less,scss,vue,html,md}\" \"vite/**/*.ts\"",
		"lint:stylelint": "stylelint \"**/*.{css,scss,vue,html}\" --fix",
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky install"
	},
	"license": "MIT",
